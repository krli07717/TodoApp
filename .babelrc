{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    ["@dr.pogodin/react-css-modules", {
      "webpackHotModuleReloading": true,
      "autoResolveMultipleImports": true,
    }],
    [
      "@babel/plugin-transform-runtime",
      {
        "regenerator": true
      }
    ],
  ]
}

